<!-- ##### Welcome Area Start ##### -->
    <div class="breadcumb-area clearfix">
        
        <!-- breadcumb content -->
        <div class="breadcumb-content">
            <div class="container h-100">
                <div class="row h-100 align-items-center">
                    <div class="col-12">
                        <nav aria-label="breadcrumb" class="breadcumb--con text-center">
                            <h2 class="title wow fadeInUp" data-wow-delay="0.2s"><%= pagetitle %></h2>
                            <ol class="breadcrumb justify-content-center wow fadeInUp" data-wow-delay="0.4s">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page"><%= pagetitle %></li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ##### Welcome Area End ##### -->

     <!-- ##### Top Sellers Start ##### -->
    <section class="about-us-area section-padding-100 clearfix">
        
        <div class="container">
            <div class="row align-items-center">
                
                
                <div class="col-12 col-lg-12">
                    <div class="who-we-contant">
                        <div class="dream-dots text-left fadeInUp" data-wow-delay="0.2s" >
                            <span class="gradient-text ">Fans</span>
                        </div>
                        <h4 class="fadeInUp" data-wow-delay="0.3s">Top Fans This Month</h4>
                    </div>
                </div>
                <div class="col-12 col-lg-4">
                    <div class="creator-sec dd-bg">
						<% topfan.forEach(function(onecat) { %>
                        <div class="author-item">
                            <div class="author-rank"><%= onecat.rank %></div>
                            <div class="author-img"><img src="<%= onecat.authorimg %>" width="70" alt=""></div>
                            <div class="author-info">
                                <a href="/profile/<%= onecat.authorid %>"><h5 class="author-name"><%= onecat.author %></h5></a>
                                <p class="author-earn mb-0"><%= onecat.authorspending %> ETH</p>
                            </div>
                        </div>
						<% }); %>
                    </div>
                </div>
                <div class="col-12 col-lg-4 mt-s">
                    <div class="creator-sec dd-bg">
                        <% topfan2.forEach(function(onecat) { %>
                        <div class="author-item">
                            <div class="author-rank"><%= onecat.rank %></div>
                            <div class="author-img"><img src="<%= onecat.authorimg %>" width="70" alt=""></div>
                            <div class="author-info">
                                <a href="/profile/<%= onecat.authorid %>"><h5 class="author-name"><%= onecat.author %></h5></a>
                                <p class="author-earn mb-0"><%= onecat.authorspending %> ETH</p>
                            </div>
                        </div>
						<% }); %>
                    </div>
                </div>
                <div class="col-12 col-lg-4 mt-s">
                    <div class="creator-sec dd-bg">
                        <% topfan3.forEach(function(onecat) { %>
                        <div class="author-item">
                            <div class="author-rank"><%= onecat.rank %></div>
                            <div class="author-img"><img src="<%= onecat.authorimg %>" width="70" alt=""></div>
                            <div class="author-info">
                                <a href="/profile/<%= onecat.authorid %>"><h5 class="author-name"><%= onecat.author %></h5></a>
                                <p class="author-earn mb-0"><%= onecat.authorspending %> ETH</p>
                            </div>
                        </div>
						<% }); %>
                    </div>
                </div>


            </div>
        </div>
    </section>
    <!-- ##### Top Authors End ##### -->

    <!-- ##### Explore Authors Start ##### -->
    <section class="section-padding-0-70 clearfix" >
        <div class="container">
            
            <div class="section-heading text-center">
                
                <div class="dream-dots justify-content-center fadeInUp" data-wow-delay="0.2s">
                    <span>Explore Stars</span>
                </div>
                <h2 class="fadeInUp" data-wow-delay="0.3s">Our Stars</h2>
                <p class="fadeInUp" data-wow-delay="0.4s">Our Star List</p>
            </div>
                

            <div class="row">
				<% topstars.forEach(function(onecat) { %>
                <div class="col-12 col-md-6 col-lg-3">
                    <!-- Content -->
                    <div  class="service_single_content collection-item">
                        <!-- Icon -->
                        <div class="collection_icon">
                            <img src="<%= onecat.lastworkimg %>" alt="">
                        </div>
                        <span class="aut-info">
                            <img src="<%= onecat.authorimg %>" width="50" alt="">
                        </span>
                        <div class="collection_info text-center">
                            <h6><%= onecat.author %></h6>
                            <p></p>
                            <a href="/profile/<%= onecat.authorid %>" class="more-btn mt-15">Visit</a>
                        </div>
                        
                    </div>
                </div>
				<% }); %>
                
            </div>
        </div>
    </section>